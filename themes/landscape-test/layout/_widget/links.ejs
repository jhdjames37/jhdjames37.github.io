<div class="widget-wrap">
  <h3 class="widget-title"><%= "links" %></h3>
    <div class="widget">
	  <% for (var col in site.data.links) { %>
	    <p> <%= col %> </p>
        <% for (var link in site.data.links[col]) { %>
          <a href="<%= site.data.links[col][link] %>"> <%= link %> </a>
        <% } %>
	  <% } %>
  </div>
</div>